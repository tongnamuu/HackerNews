uter"},{"generated":{"line":46,"column":44},"source":"app.js","original":{"line":80,"column":44}},{"generated":{"line":46,"column":45},"source":"app.js","original":{"line":80,"column":45}},{"generated":{"line":47,"column":0},"source":"app.js","original":{"line":81,"column":0},"name":"router"},{"generated":{"line":47,"column":6},"source":"app.js","original":{"line":81,"column":6}},{"generated":{"line":47,"column":7},"source":"app.js","original":{"line":81,"column":7}},{"generated":{"line":47,"column":8},"source":"app.js","original":{"line":81,"column":8}}],"sources":{"app.js":"const ajax= new XMLHttpRequest();\nconst URL = 'https://api.hnpwa.com/v0/news/1.json'\nconst CONTENT_URL = 'https://api.hnpwa.com/v0/item/@id.json'\nconst content = document.createElement(\"div\")\nconst container = document.getElementById(\"root\")\n\nconst ul = document.createElement('ul');\ncontainer.appendChild(ul)\ncontainer.appendChild(content)\n\nconst store = {\n    currentPage: 1\n}\n\nfunction newsFeed() {\n    const newsFeed = getData(URL)\n    const newsList = []\n    let template = `\n        <div>\n            <h1>Hacker News</h1>\n            <ul>\n                {{__news_feed__}}\n            </ul>\n            <div>\n                <a href='#/page/{{__prev_page__}}'>이전페이지</a>\n                <a href='#/page/{{__next_page__}}'>다음페이지</a>\n            </div>\n        </div>\n    `\n\n\n    for(let i=(store.currentPage-1) * 10 ; i<store.currentPage*10 ;i++) {\n    newsList.push(\n        `\n        <li>\n          <a href=\"#/show/${newsFeed[i].id}\">\n          ${newsFeed[i].title}  ${newsFeed[i].comments_count}\n          </a\n        </li>\n    `  \n    )\n    }\n    template = template.replace('{{__news_feed__}}', newsList.join(''))\n    template = template.replace('{{__prev_page__}}', store.currentPage > 1 ? store.currentPage - 1 : 1)\n    template = template.replace('{{__next_page__}}', store.currentPage*10 >= newsFeed.length ? store.currentPage : store.currentPage + 1)\n    container.innerHTML = template\n}\n\nfunction newsContent(){\n    const id = location.hash.substring(7)\n    const newsContent = getData(CONTENT_URL.replace('@id', id))\n\n    container.innerHTML = `\n      <h1>${newsContent.title}</h1>\n      <div>\n        <a href=\"#/page/${store.currentPage}\">목록으로</a>\n      </div>\n    `;\n}\n\nfunction router() {\n    const routePath = location.hash;\n    console.log(routePath)\n    if(routePath === '') {\n        newsFeed()\n    } else if(routePath.indexOf('#/page') != -1) {\n        store.currentPage = Number(location.hash.substring(7))\n        newsFeed()\n    } else {\n        newsContent()\n    }\n}\n\nfunction getData(url) {\n    ajax.open('GET', url, false)\n    ajax.send()\n    return JSON.parse(ajax.response)\n}\n\nwindow.addEventListener('hashchange', router)\nrouter()"},"lineCount":null}},"error":null,"hash":"bd424ac0aac14565f4df294e3e6dffce","cacheData":{"env":{}}}